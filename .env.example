# OpenAI
# 필수: OpenAI API 키
OPENAI_KEY=

# 선택: 모델 오버라이드 (기본값은 코드 내 기본값 사용)
# 채팅/완성 모델 (예: gpt-5-mini, gpt-4o-mini 등)
OPENAI_CHAT_MODEL=
# 모더레이션 모델 (예: omni-moderation-latest)
OPENAI_MODERATION_MODEL=
# 임베딩 모델 (예: text-embedding-3-small, text-embedding-3-large)
OPENAI_EMBEDDING_MODEL=

# Supabase
# 필수: 프로젝트 URL
NEXT_PUBLIC_SUPABASE_URL=
# 필수: 서비스 롤 키 (서버 전용, 절대 클라이언트에 노출하지 마세요)
SUPABASE_SERVICE_ROLE_KEY=
# 로컬 개발 시 필요할 수 있음 (클라이언트용 익명 키)
NEXT_PUBLIC_SUPABASE_ANON_KEY=

LANGSMITH_TRACING=true
LANGSMITH_API_KEY=sk_your_langsmith_key
LANGSMITH_PROJECT=nextjs-openai-doc-search
